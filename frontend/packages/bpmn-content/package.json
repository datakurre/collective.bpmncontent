{
  "name": "volto-bpmn-content",
  "version": "1.0.0-alpha.0",
  "description": "A BPMN Content Management Add-on for Plone",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "volto-addon",
    "volto",
    "plone",
    "react"
  ],
  "author": "Asko Soukka",
  "homepage": "https://github.com/datakurre/bpmn-content#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:datakurre/bpmn-content.git"
  },
  "publishConfig": {
